<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.termtracker">

    <application
        android:name=".TermTracker"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MentorSelectActivity"></activity>
        <activity android:name=".CourseMentorsActivity" />
        <activity android:name=".MentorDetailActivity" />
        <activity android:name=".MentorEditActivity" />
        <activity android:name=".NoteDetailActivity" />
        <activity android:name=".NoteEditActivity" />
        <activity android:name=".AlertActivity" />
        <activity android:name=".CourseAlertEditActivity" />
        <activity android:name=".AssessmentAlertEditActivity" />
        <activity android:name=".AssessmentEditActivity" />
        <activity android:name=".AssessmentDetailActivity" />
        <activity android:name=".AssessmentActivity" />
        <activity android:name=".CourseEditActivity" />
        <activity android:name=".CourseDetailActivity" />
        <activity android:name=".CourseActivity" />
        <activity android:name=".TermEditActivity" />
        <activity android:name=".TermDetailActivity" />
        <activity
            android:name=".TermActivity"
            android:label="@string/title_activity_term"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.termtracker.MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserSetupActivity" />

        <receiver android:name=".util.AlertReceiver"/>

    </application>

</manifest>